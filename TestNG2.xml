<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Test Suite - Log In" parallel="tests" thread-count="2">
    <test name="Log In Tests - Chrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="LogInTests" />
        </classes>
    </test>
    <test name="Log In Tests - Edge">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="LogInTests" />
        </classes>
    </test>
    <test name="Log In Tests - Firefox">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="LogInTests" />
        </classes>
    </test>
</suite>
